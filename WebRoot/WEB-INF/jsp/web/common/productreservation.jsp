<input type="hidden" id="PRODUCTID" value="">
<input type="hidden" id="PRODUCTTYPE" value="">
<script>
	$(function(){
		$("#clous").click(function (event) {
                $(".yy1").hide();
        });
		$('#ljyy').click(function(){
			var data={};
			if (!$("#NAMES").val().trim()) { 
				alert("请输入您的姓名！"); 
				$("#NAMES").focus(); 
				return false; 
			} 
			data.NAME = $("#NAMES").val();
			if (!$("#PHONE").val().match(/^(((1[0-9]{2})|159|153)+\d{8})$/)) {
				alert("手机号码格式不正确！"); 
				$("#PHONE").focus(); 
				return false; 
			}
			data.PHONE = $("#PHONE").val();
			if(Number($("#MONEY").val())>0&&!isNaN($("#MONEY").val())){
				data.MONEY = $("#MONEY").val();
			}else{
				alert("请输入有效金额！");
				return;
			}
			if($("#PRODUCTID").val()){
				data.PRODUCTID = $("#PRODUCTID").val();
			}
			if($("#PRODUCTTYPE").val()){
				data.TYPE = $("#PRODUCTTYPE").val();
			}
			$.ajax({
					type: "POST",
					url: '<%=basePath%>web/cpyy',
			    	data: data,
					dataType:'json',
					success: function(data){
						 if(data.result == 01){
						 	alert("预约成功");
						 	$(".yy1").hide();
						 	$("#MONEY").val('');
						 }else{
						 	alert(data.msg);
						 }
					}
			});
		});
		
	})
</script>